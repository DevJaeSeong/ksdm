}*/

.notice_info a:hover::before {
    background-color: #222;
}
.notice_info .notice_date{position: absolute; right: 0; font-size: 14px; }
.user_icon {
    display: inline-block;
    width: 415px;
    height: 274px;
    float: right;
}

.notice_info >p {font-size: 14px; font-family: 'Noto2';}
.notice_info >p:last-child::after {
    content: '';
    background-image: url(../images/scenario_icon.png);
    background-position: center;
    background-repeat: no-repeat;
    background-size: 100%;
    position: absolute;
    top: 70%; right: 20px;
    transform: translateY(-50%);
    width: 100px;
    height: 100px;
}
.h_notice:hover {background-color: #005fb1; color: #fff;}

.h_notice:hover a::after {background-image: url(../images/plus_bt_hover.png);opacity: 1;}

.user_icon>a {
    display: inline-block;
    width: calc(100% - 24px);
    height: 105px;
    padding: 20px 0 0 24px;
    background-color: #ffffff;
    transition: 0.3s;
}

.user_icon>a:hover {
    background-color: #005fb1;
}

.user_icon>a:hover> * {
    color: #fff;
}

.user_icon>a:first-child {
    margin-bottom: 15px;
    position: relative;
}

.user_icon>a:first-child::after {
    content: '';
    background-image: url(../images/manual_icon.png);
    background-position: center;
    background-repeat: no-repeat;
    background-size: 100%;
    position: absolute;
    top: 50%; right: 20px;
    transform: translateY(-50%);
    width: 100px;
    height: 100px;
}

.user_icon>a:last-child {
    position: relative;
    height: 100%;
}


.user_icon>a>p {
    display: inline-block;
    float: left;
    position: relative;
}

.user_icon>a>p:first-child {
    margin-bottom: 10px;
}

.user_icon>a>p:first-child>span {
    display: inline-block;
    float: left;
    font-family: 'Noto6';
    font-size: 18px;
}

.user_bar {
    width: 225px;
    height: 2px;
    background-color: #cccccc;
    margin: 12px 10px 0 10px;
    transition: 0.3s;
}

.user_icon>a:hover .user_bar {
    background-color: #fff;
}

.user_icon>a>p:first-child::after {
    content: '';
    background-image: url(../images/plus_bt.png);
    background-position: center;
    background-repeat: no-repeat;
    background-size: 100%;
    position: absolute;
    top: 50%; right: -20px;
    transform: translateY(-50%);
    width: 20px;
    height: 20px;
    opacity: 0.5;
    transition: 0.3s;
}

.user_icon>a:hover>p:first-child::after {
    background-image: url(../images/plus_bt_hover.png);
    opacity: 1;
}

.user_icon>a>p:last-child>span {
    display: inline-block;
    font-family: 'Noto5';
    font-size: 12px;
    float: left;
    margin-right: 30px;
}


/* ---------- sec2 끝 ---------- */

