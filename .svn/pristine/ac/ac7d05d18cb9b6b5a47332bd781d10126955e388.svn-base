@charset "utf-8";

/* ---------------------------------------------------------- */
.wrap { width: 100%; height: 937px; } 
header { position: fixed; top: 0; left: 0; display: flex; justify-content: space-between; align-items: center; flex-wrap: nowrap; width: 100%; /*max-width: 1300px;*/ height: 72px; background-color: #2d4497; white-space: nowrap; z-index: 5; } 
/* header::after {content: ""; background-color: #2d4497; width: 200%; height: 72px; position: absolute; left: 0; z-index: -1;} */
header>h1 { width: 277px; height: 100%; display: inline-block; /* float: left; */}
header>h1>a { display: flex; justify-content: center; align-items: center; width: 100%; height: 100%; } 
header>a { display: inline-block; /* float: right; */
 position: relative; padding: 35px 45px 35px 26px; background-color: #112773; color: white; font-family: 'Noto4'; font-size: 14px; } 
header>a::after { content: ''; position: absolute; top: 50%; right: 24px; transform: translateY(-50%); width: 14px; height: 14px; background-image: url(../images/arrow_icon.png); background-position: center; background-repeat: no-repeat; background-size: 100%; } 
header .go_service:hover { background: #405fff; } 
/*========== 사이드메뉴 시작 ==========*/
.gnb { position: fixed; top: 72px; left: 0; width: 277px; height: 100%; background-color: #434d67; z-index: 5;}
.aside { display: inline-block; width: 277px; } 
/* ----- 정보창 시작 ----- */
.user_info { display: inline-block; width: 277px; height: 200px; text-align: center; background-color: #2d3343; background-repeat: no-repeat; } 
.user_info>li:first-child { display: inline-block; float: left; width: 100%; height: 160px; } 
.photo { display: inline-block; float: left; width: 210px; height: 100%; } 
.photo>div { display: inline-block; float: left; padding: 48px 10px 47px 25px; } 
.photo>p { display: flex; float: left; justify-content: center; align-items: center; flex-direction: column; width: 100px; height: 100%; } 
.photo>p>span { display: inline-block; line-height: 1.5em; width: 100%; text-align: left; font-size: 14px; color: white; } 
.photo>p>span:last-child { font-weight: bold; } 
.notice { display: inline-block; float: right; width: 65px; height: 100%; background-color: #434c63; } 
.notice>a { display: inline-block; width: 100%; height: 50%; position: relative; font-family: 'SC'; font-weight: 400; font-size: 14px; line-height: 130px; color: white; transition: 0.3s; } 
.notice>a>span { line-height: 1.5em; position: absolute; font-size: 10px; font-weight: bold; font-family: 'Noto3'; padding: 0 7px; top: 5px; right: 5px; background-color: #405fff; border-radius: 10px; transition: 0.3s; } 
.notice>a:hover>span { background-color: #ffffff; color: #b0282a; } 
.notice>a:hover { background-color: #405fff; } 
.notice>a:first-child { border-bottom: 1px solid #1e232e; } 
.notice>a:first-child::before { content: ''; position: absolute; top: 40%; left: 50%; transform: translate(-50%, -50%); width: 40px; height: 40px; background-image: url(../images/notice_icon.png); background-position: center; background-size: 100%; background-repeat: no-repeat; transition: 0.3s; } 
/*.notice>a:first-child:hover::before { background-image: url(../images/notice_icon_hover.png); } */
.notice>a:last-child::before { content: ''; position: absolute; top: 40%; left: 50%; transform: translate(-50%, -50%); width: 35px; height: 35px; background-image: url(../images/message_icon.png); background-position: center; background-size: 100%; background-repeat: no-repeat; transition: 0.3s; } 
/*.notice>a:last-child:hover::before { background-image: url(../images/message_icon_hover.png); } */
.user_info>li:last-child { display: inline-block; float: left; width: 100%; height: 40px; background-color: #3a4056; } 
.user_info>li:last-child > a { display: inline-block; width: calc(50% - 2px); height: calc(100% - 2px); float: left; line-height: 2.5em; color: white; font-family: 'Noto5'; font-size: 14px; border: 1px solid #1e232e; transition: 0.3s; } 
.user_info>li:last-child > a:hover { background-color: #b0282a; } 
/* ----- 정보창 끝 ----- */
.accordion { width: 100%; background: #FFF; } 
.accordion .link { width: 100%; height: 50px; cursor: pointer; display: inline-block; color: #575757; border-bottom: 1px solid #28384a; position: relative; -webkit-transition: all 0.4s ease; -o-transition: all 0.4s ease; transition: all 0.4s ease; background-color: #3a4056; font-family: 'SC'; font-size: 14px;}
.accordion li:first-child { border-top: 2px solid #28384a; } 
.accordion li p { position: relative; display: inline-block; height: 100%; width: calc(100% - 20px); text-align: center; line-height: 3.5em; color: #ffffff; text-align: left; margin-left: 20px; } 
.accordion li p::after { content: ''; position: absolute; background-image: url(../images/menu_plus.png); background-repeat: no-repeat; background-size: 100%; background-position: center; top: 50%; right: 20px; transform: translateY(-50%); width: 20px; height: 20px; -webkit-transition: all 0.4s ease; -o-transition: all 0.4s ease; transition: all 0.4s ease; opacity: 1; } 
.accordion li.open .link { background-color: #405fff; color: #145ba2; border-bottom: 0px; } 
.accordion li.open p::after { opacity: 1; background-image: url(../images/menu_minus.png); top: 50%; right: 20px; transform: translateY(-50%) rotate(180deg); } 
/*----- dep2 시작 -----*/
.submenu { display: none; font-size: 14px; position: relative; z-index: 10; } 
.submenu li { /* border-bottom: 1px solid #ccc; */
 display: inline-block; height: 43px; width: 100%; } 
.submenu a { display: inline-block; height: 100%; padding-left: 20px; width: calc(100% - 22px); line-height: 3em; -webkit-transition: all 0.25s ease; -o-transition: all 0.25s ease; transition: all 0.25s ease; font-family: 'SC'; font-weight: 200; font-size: 13px; background-color: #2d3343; color: #ffffff; border: 1px solid #151d26; } 
.submenu a:hover { background: #2c3f84; } 
/*----- dep2 끝 -----*/
