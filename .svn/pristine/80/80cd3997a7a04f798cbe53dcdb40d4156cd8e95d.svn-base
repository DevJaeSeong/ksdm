.rocate { display: inline-block; width: 60%; padding: 20px 0; margin-left: 23px; } 
.rocate img { width: 16px; /*vertical-align: middle; */}
.rocate ul li { display: inline-block; margin-right: 5px; font-family: 'SC'; font-weight: 400; font-size: 15px; color: #a6a6a6; } 
.rocate ul li::after { content: ""; background: url(../images/i_arrow_r.png) no-repeat; display: inline-block; width: 8px; height: 11px; background-size: 100% 100%; position: relative; right: -5px; } 
.rocate ul li.ro_last::after { content: ""; border: 1px solid red; display: none; } 
.ro_last span { color: #131313; font-weight: 500; } 

.search { display: block; margin-left: 25px; width: 1592px; height: 50px; background-color: #e6ebf2; } 
.search_txt>span { position: relative; top: 25px; } 
.search_txt>span { font-family: 'SC'; font-weight: 500; font-size: 18px; color: #333; margin-left: 10px; } 
.search_box { float: right; width: 250px; height: 100%; margin-left: 150px; position: relative; } 
.search_box input { position: absolute; top: 8px; left: 0; width: 200px; height: 30px; padding-left: 5px; border: 1px solid #e0e0e0; } 
.search_click img {cursor: pointer; width: 32px; height: 32px; position: relative; top: 8px; left: 206px; } 
.search_box_02 {float: right;margin-right: 25px;position: relative;top: 5px;width: 290px; }
.search_box_02 input {position: absolute; top: 8px; right: 55px; width: 200px; height: 31px; padding-left: 5px; border: 1px solid #e0e0e0; } 
.search_click_02{cursor: pointer;cursor: pointer;background-color: #2d4497;color: #fff;padding: 8px 15px;position: absolute;top: 8px;right: 0px;box-shadow: none;}

