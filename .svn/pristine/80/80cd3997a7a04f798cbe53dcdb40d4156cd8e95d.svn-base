.title { display: inline-block; width: 100%; } 
.title p { display: block; padding: 10px 0; } 
.title p:first-child { border-bottom: 1px solid #cccccc; font-family: 'SC'; font-weight: 500; font-size: 18px; /* border: 1px solid red; */
 display: flex; justify-content: space-between; align-items: center; } 
.title p:first-child>a { display: inline-block; float: right; width: 20px; height: 20px; opacity: 0.5; transition: 0.3s; } 
.title p:first-child>a:hover { opacity: 1; } 
.title p:first-child>a>img { width: 100%; } 
.title p:last-child { font-family: 'SC'; font-size: 14px; padding: 10px 0; line-height: 1.5em; } 
.title p:last-child>span { color: #ff3400; font-weight: bold; } 
